msgid "IDS_PCEE670_HEROES_FLAG"
msgstr ""
"Heroes of\n"
"Might and Magic III"

msgid "IDS_PCEE670_HEROES_FLAG_DESCRIPTION"
msgstr "「埃拉西亞的旗幟必須受到尊重，絕不能被玷汙。埃拉西亞不能落入敵人之手！」"

msgid "IDS_PCEE670_HEROES_FLAG_EXTRA_DESCRIPTION"
msgstr "與《Heroes of Might and Magic III》的合作。"

msgid "IDS_PCEE671_XBOX_FLAG"
msgstr "Xbox"

msgid "IDS_PCEE671_XBOX_FLAG_DESCRIPTION"
